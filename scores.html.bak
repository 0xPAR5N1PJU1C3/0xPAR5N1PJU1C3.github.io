<!DOCTYPE html>

<html>

<body>

<div>
  <canvas id="myChart"></canvas>
</div>

<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<script>
  const labels = [
    '@cathoderaytub',
    '@VideoLiving',
    '@DarpaChief',
    '@Twentynil',
    '@eric',
  ];

  const data = {
    labels: labels,
    datasets: [{
      label: 'Achievement Points',
      backgroundColor: '#F07557',
      borderColor: '#F07557',
      data: [6, 0, 2, 3, 4],
    }]
  };

  const config = {
    type: 'bar',
    data: data,
    options: {}
  };
</script>

<script>
  const myChart = new Chart(
    document.getElementById('myChart'),
    config
  );
</script>

</body>
</html>
